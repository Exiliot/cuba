{
  "name": "cuba",
  "version": "0.1.6",
  "description": "Google Sheets + SQL = JSON",
  "main": "src",
  "author": "Lim Yuan Qing",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/cuba.git"
  },
  "bin": {
    "cuba": "src/cli.js"
  },
  "dependencies": {
    "JSONStream": "^1.3.2",
    "duplexer": "^0.1.1",
    "isomorphic-unfetch": "^2.0.0",
    "jws": "^3.1.4",
    "nopt": "^4.0.1"
  },
  "devDependencies": {
    "browserify": "^16.2.0",
    "concat-stream": "^1.6.2",
    "dotenv": "^5.0.1",
    "gzip-size-cli": "^2.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "nyc": "^11.6.0",
    "prettier-standard": "^8.0.1",
    "rimraf": "^2.6.2",
    "standard": "^11.0.1",
    "tape": "^4.9.0",
    "uglify-es": "^3.3.9"
  },
  "scripts": {
    "build": "rimraf umd.js && browserify src/index.js --standalone cuba | uglifyjs --compress --mangle --toplevel > umd.js",
    "clean": "rimraf '*.log' .nyc_output coverage umd.js",
    "coverage": "rimraf .nyc_output coverage && nyc --reporter=html --reporter=lcov --reporter=text npm test",
    "fix": "prettier-standard 'example/**/*.js' 'src/**/*.js'",
    "lint": "standard 'example/**/*.js' 'src/**/*.js'",
    "precommit": "lint-staged",
    "test": "tape --require dotenv/config 'src/**/__tests__/*.js'",
    "weight": "browserify src/micro/index.js --standalone cuba | uglifyjs --compress --mangle --toplevel | gzip-size"
  },
  "lint-staged": {
    "*.js": [
      "standard",
      "git add"
    ]
  },
  "files": [
    "src",
    "umd.js"
  ],
  "unpkg": "umd.js",
  "keywords": [
    "api",
    "google",
    "google-api",
    "google-sheet",
    "google-sheets",
    "google-spreadsheet",
    "google-spreadsheets",
    "sheet",
    "spreadsheet"
  ]
}
