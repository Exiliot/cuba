{
  "name": "cuba",
  "version": "0.1.12",
  "description": "Google Sheets + SQL = JSON",
  "author": "Lim Yuan Qing",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/cuba.git"
  },
  "browser": "array.js",
  "bin": {
    "cuba": "src/cli.js"
  },
  "dependencies": {
    "JSONStream": "^1.3.2",
    "duplexer": "^0.1.1",
    "google-auth-library": "^1.5.0",
    "node-fetch": "^2.1.2",
    "nopt": "^4.0.1"
  },
  "devDependencies": {
    "browserify": "^16.2.2",
    "concat-stream": "^1.6.2",
    "gzip-size-cli": "^2.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.1",
    "localenv": "^0.2.2",
    "localenvify": "^1.0.1",
    "nyc": "^11.8.0",
    "prettier-standard": "^8.0.1",
    "rimraf": "^2.6.2",
    "standard": "^11.0.1",
    "tape": "^4.9.0",
    "tape-run": "^4.0.0",
    "uglify-es": "^3.3.9"
  },
  "scripts": {
    "clean": "rimraf '*.log' .nyc_output coverage",
    "coverage": "rimraf .nyc_output coverage && nyc --reporter=html --reporter=lcov --reporter=text yarn test",
    "fix": "prettier-standard 'example/**/*.js' 'src/**/*.js'",
    "lint": "standard 'example/**/*.js' 'src/**/*.js'",
    "precommit": "lint-staged",
    "test": "yarn test-browser && yarn test-node",
    "test-browser": "browserify --transform localenvify src/array/__tests__/index.js | tape-run",
    "test-node": "tape --require localenv 'src/**/__tests__/*.js'",
    "weight": "browserify ./ --standalone cuba | uglifyjs --compress --mangle --toplevel | gzip-size"
  },
  "lint-staged": {
    "*.js": [
      "standard",
      "git add"
    ]
  },
  "files": [
    "src",
    "array.js",
    "index.js",
    "stream.js"
  ],
  "keywords": [
    "api",
    "google",
    "google-api",
    "google-sheet",
    "google-sheets",
    "google-spreadsheet",
    "google-spreadsheets",
    "sheet",
    "spreadsheet"
  ]
}
