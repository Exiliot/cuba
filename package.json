{
  "name": "cuba",
  "version": "0.1.3",
  "description": "Google Sheets + SQL = JSON",
  "author": "Lim Yuan Qing",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/cuba.git"
  },
  "bin": {
    "cuba": "cli.js"
  },
  "dependencies": {
    "JSONStream": "^1.3.2",
    "duplexer": "^0.1.1",
    "isomorphic-unfetch": "^2.0.0",
    "jws": "^3.1.4",
    "nopt": "^4.0.1"
  },
  "devDependencies": {
    "concat-stream": "^1.6.2",
    "cpy-cli": "^1.0.1",
    "mkdirp": "^0.5.1",
    "nyc": "^11.6.0",
    "prettier-standard": "^8.0.1",
    "rimraf": "^2.6.2",
    "standard": "^11.0.1",
    "tape": "^4.9.0"
  },
  "scripts": {
    "build": "rimraf build && yarn clean && mkdirp build && cpy '**/*.*' ../build --cwd=src --parents && cpy LICENSE.md package.json README.md build",
    "clean": "rimraf '*.log' .nyc_output build coverage",
    "coverage": "rimraf coverage && nyc --reporter=html --reporter=lcov --reporter=text npm test",
    "fix": "prettier-standard 'example/**/*.js' 'src/**/*.js' 'test/**/*.js'",
    "lint": "standard 'example/**/*.js' 'src/**/*.js' 'test/**/*.js'",
    "test": "tape 'test/*.js'"
  },
  "keywords": [
    "api",
    "google",
    "google-api",
    "google-sheet",
    "google-sheets",
    "google-spreadsheet",
    "google-spreadsheets",
    "sheet",
    "spreadsheet"
  ]
}
